    "def get_impo(df):\n",
    "    return ( \"####### BASIC INFORMATION#####\",df.info(),\n",
    "            \"######FIRST FIVE ROWS######\",df.head(),\n",
    "           \"###### NULL VALUES ###########\",df.isnull().sum(),\n",
    "            \"###### STATICS ########\",df.describe(),\n",
    "            \"####### SHAPE##########\",df.shape)\n",
    "\n"
   ]
  },
  {
