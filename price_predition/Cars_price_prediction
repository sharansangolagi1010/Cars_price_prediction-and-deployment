    "for i in range(1000):\n",
    "    X_train,X_test,y_train,y_test=train_test_split(df.drop(\"Price\",axis=1),df[\"Price\"],test_size=(0.3),random_state=(i))\n",
    "    pip=Pipeline([(\"tr1\",tr1),(\"lr\",lr)])\n",
    "    pip.fit(X_train,y_train)\n",
    "    y_pred=pip.predict(X_test)\n",
    "    scores.append(r2_score(y_test,y_pred))\n",
    "    \n",
    "    \n",
    "    \n"
   ]
  },
